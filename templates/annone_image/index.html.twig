{% extends 'base.html.twig' %}

{% block title %}AnnoneImage index{% endblock %}

{% block body %}
    <h1>AnnoneImage index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for annone_image in annone_images %}
            <tr>
                <td>{{ annone_image.id }}</td>
                <td>
                    <a href="{{ path('app_annone_image_show', {'id': annone_image.id}) }}">show</a>
                    <a href="{{ path('app_annone_image_edit', {'id': annone_image.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_annone_image_new') }}">Create new</a>
{% endblock %}
